{% macro prototype_ligne_colonne_info(form) %}
    <div class="list-colonne_info"  data-prototype="{{ _self.colonne(form.infoEtudiants.vars.prototype)|e }}"></div>
    {# <button type="button" data-protoname="__name__" class="btn btn-default add_line_info btn-xs" id="add_ligne_info"  data-protoclass=".list-colonne_info" data-container=".column-container_info">
        <i class="fa fa-plus"></i> Ajouter une ligne
    </button> #}
    <div class="column-container_info proto-container_info" data-childclass="">
        {% for item in form.infoEtudiants %}
            {{ _self.colonne(item|e) }}
        {% endfor %}
    </div>
{% endmacro %}